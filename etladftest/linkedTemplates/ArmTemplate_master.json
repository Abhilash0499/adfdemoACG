{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "etladftest"
		},
		"LS_azuresqlserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_azuresqlserver'"
		},
		"LS_blobstorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_blobstorage'"
		},
		"LS_blobstorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageetltest.dfs.core.windows.net/"
		},
		"LS_blobusingkyvlt_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "myblob"
		},
		"LS_sqlusingkyvlt_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "mysqldb"
		},
		"lnkkeyvlt_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://etltestkeyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "etladftest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_azuresqlserver_connectionString": {
						"value": "[parameters('LS_azuresqlserver_connectionString')]"
					},
					"LS_blobstorage_accountKey": {
						"value": "[parameters('LS_blobstorage_accountKey')]"
					},
					"LS_blobstorage_properties_typeProperties_url": {
						"value": "[parameters('LS_blobstorage_properties_typeProperties_url')]"
					},
					"lnkkeyvlt_properties_typeProperties_baseUrl": {
						"value": "[parameters('lnkkeyvlt_properties_typeProperties_baseUrl')]"
					},
					"LS_blobusingkyvlt_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_blobusingkyvlt_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_sqlusingkyvlt_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_sqlusingkyvlt_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}